<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="detail">
	
    <select id="detaillist"  resultType="com.ict.project.vo.TdetailVO">
    	select * from testdetail where d_title = #{area}
  	</select>
  	
    <select id="locallist"  resultType="com.ict.project.vo.TdetailVO">
    	select * from testdetail where d_local = #{local}
  	</select>
  	
		
</mapper>
